# Output random alphanumeric strings.
# Speficy length of string and number of outputs.

import random
import json
import requests

chars = 32
times = 1
accountEmpty = true

d = 'abcdefghijklmnopqrstuvwxyz1234567890'
word = ''
letter = ''
while (accountEmpty):
while times > 0:
    for i in range(chars):
        letter = d[random.randint(0, len(d) -1)]
        word = word + letter
