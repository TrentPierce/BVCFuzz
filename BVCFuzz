# Output random alphanumeric strings.
# Speficy length of string and number of outputs.

import random
import json
import requests

chars = 32
accountEmpty = True

d = 'abcdefghijklmnopqrstuvwxyz1234567890'
word = ''
letter = ''
while accountEmpty:
    for i in range(chars):
        letter = d[random.randint(0, len(d) -1)]
        word = word + letter
    response = requests.get("https://bitcoinvideocasino.com/account/balance?account_key=" + word)
    data = response.json()
    if "message" in data:
     print("https://bitcoinvideocasino.com/account/balance?account_key=" + word + " Balance: " + message)
     accountEmpty = False
    else:
     print(word)
     print(data)
    word = ''
